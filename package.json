{
    "name": "flash-docusaurus",
    "displayName": "FlashDocusaurus",
    "description": "A VS Code extension for Docusaurus documentation development with intelligent features.",
    "version": "0.0.1",
    "publisher": "FlashDocs",
    "private": true,
    "license": "MIT",
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Formatters"
    ],
    "keywords": [
        "Docusaurus",
        "MDX",
        "markdown",
        "documentation"
    ],
    "activationEvents": [
        "onLanguage:markdown",
        "onLanguage:mdx"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "FlashDocusaurus",
            "properties": {
                "flashDocusaurus.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable FlashDocusaurus extension features"
                }
            }
        },
        "commands": [
            {
                "command": "flashDocusaurus.helloWorld",
                "title": "Hello World",
                "category": "FlashDocusaurus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint \"src/**/*.ts\""
    },
    "devDependencies": {
        "@types/node": "^16.18.34",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "eslint": "^8.26.0",
        "typescript": "^5.3.2"
    }
}

